<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.0.0">
    <servers>
        <server>
            <id>central</id>
            <username>${env.MAVEN_REPO_USER}</username>
            <password>${env.MAVEN_REPO_PASS}</password>
        </server>
        <server>
            <id>snapshots</id>
            <username>${env.MAVEN_REPO_USER}</username>
            <password>${env.MAVEN_REPO_PASS}</password>
        </server>
    </servers>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
    <profiles>
        <profile>
            <id>artifactory</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <name>artifactory-central</name>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <url>${env.MAVEN_REPO_URL}/libs-release</url>
                </repository>
                <repository>
                    <id>snapshots</id>
                    <name>artifactory-snapshot</name>
                    <url>${env.MAVEN_REPO_URL}/libs-snapshot</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <name>artifactory-central</name>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <url>${env.MAVEN_REPO_URL}/libs-release</url>
                </pluginRepository>
                <pluginRepository>
                    <id>snapshots</id>
                    <name>artifactory-snapshot</name>
                    <url>${env.MAVEN_REPO_URL}/libs-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
